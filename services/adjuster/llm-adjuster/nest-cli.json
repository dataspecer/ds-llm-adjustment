{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/llm-adjuster/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/llm-adjuster/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/llm-adjuster",
  "projects": {
    "llm-adjuster": {
      "type": "application",
      "root": "apps/llm-adjuster",
      "entryFile": "main",
      "sourceRoot": "apps/llm-adjuster/src",
      "compilerOptions": {
        "tsConfigPath": "apps/llm-adjuster/tsconfig.app.json"
      }
    },
    "dialogs-handler": {
      "type": "application",
      "root": "apps/dialogs-handler",
      "entryFile": "main",
      "sourceRoot": "apps/dialogs-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dialogs-handler/tsconfig.app.json"
      }
    },
    "changes-detector": {
      "type": "application",
      "root": "apps/changes-detector",
      "entryFile": "main",
      "sourceRoot": "apps/changes-detector/src",
      "compilerOptions": {
        "tsConfigPath": "apps/changes-detector/tsconfig.app.json"
      }
    },
    "changes-suggester": {
      "type": "application",
      "root": "apps/changes-suggester",
      "entryFile": "main",
      "sourceRoot": "apps/changes-suggester/src",
      "compilerOptions": {
        "tsConfigPath": "apps/changes-suggester/tsconfig.app.json"
      }
    },
    "dataspecer-adapter": {
      "type": "application",
      "root": "apps/dataspecer-adapter",
      "entryFile": "main",
      "sourceRoot": "apps/dataspecer-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dataspecer-adapter/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}